<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="900.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.fx.Login_Controller">
   <children>
      <Pane fx:id="RootPane" layoutX="500.0" layoutY="250.0" onMouseClicked="#exit" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" layoutX="122.0" layoutY="78.0" prefHeight="64.0" prefWidth="353.0" text="로그인 창" />
            <TextField fx:id="field_id" layoutX="180.0" layoutY="152.0" prefHeight="22.0" prefWidth="227.0" />
            <TextField fx:id="field_pw" layoutX="180.0" layoutY="189.0" prefHeight="22.0" prefWidth="227.0" />
            <Button fx:id="login_btn" layoutX="210.0" layoutY="325.0" mnemonicParsing="false" onAction="#login" prefHeight="58.0" prefWidth="172.0" text="로그인" />
            <Button fx:id="register_btn" layoutX="396.0" layoutY="325.0" mnemonicParsing="false" onAction="#register" prefHeight="58.0" prefWidth="172.0" text="회원가입" />
         </children>
      </Pane>
   </children>
</AnchorPane>
